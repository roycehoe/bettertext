#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/bin/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/js-yaml@4.1.0/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules:/home/socrates/ghq/github.com/roycehoe/node_modules:/home/socrates/ghq/github.com/node_modules:/home/socrates/ghq/node_modules:/home/socrates/node_modules:/home/node_modules:/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/@eslint+eslintrc@1.0.5/node_modules/@eslint/eslintrc/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/@eslint+eslintrc@1.0.5/node_modules/@eslint/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/@eslint+eslintrc@1.0.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/bin/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/js-yaml@4.1.0/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules:/home/socrates/ghq/github.com/roycehoe/node_modules:/home/socrates/ghq/github.com/node_modules:/home/socrates/ghq/node_modules:/home/socrates/node_modules:/home/node_modules:/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/@eslint+eslintrc@1.0.5/node_modules/@eslint/eslintrc/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/@eslint+eslintrc@1.0.5/node_modules/@eslint/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/@eslint+eslintrc@1.0.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../js-yaml@4.1.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../../../../../../js-yaml@4.1.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
fi
