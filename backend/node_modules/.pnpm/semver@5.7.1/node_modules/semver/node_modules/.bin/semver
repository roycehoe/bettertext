#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/semver@5.7.1/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules:/home/socrates/ghq/github.com/roycehoe/node_modules:/home/socrates/ghq/github.com/node_modules:/home/socrates/ghq/node_modules:/home/socrates/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/semver@5.7.1/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules:/home/socrates/ghq/github.com/roycehoe/node_modules:/home/socrates/ghq/github.com/node_modules:/home/socrates/ghq/node_modules:/home/socrates/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/semver" "$@"
else
  exec node  "$basedir/../../bin/semver" "$@"
fi
