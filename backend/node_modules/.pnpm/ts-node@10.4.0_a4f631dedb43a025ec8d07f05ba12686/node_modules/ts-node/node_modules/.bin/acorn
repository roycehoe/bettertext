#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/acorn@8.7.0/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules:/home/socrates/ghq/github.com/roycehoe/node_modules:/home/socrates/ghq/github.com/node_modules:/home/socrates/ghq/node_modules:/home/socrates/node_modules:/home/node_modules:/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/ts-node@10.4.0_a4f631dedb43a025ec8d07f05ba12686/node_modules/ts-node/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/ts-node@10.4.0_a4f631dedb43a025ec8d07f05ba12686/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/acorn@8.7.0/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules:/home/socrates/ghq/github.com/roycehoe/node_modules:/home/socrates/ghq/github.com/node_modules:/home/socrates/ghq/node_modules:/home/socrates/node_modules:/home/node_modules:/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/ts-node@10.4.0_a4f631dedb43a025ec8d07f05ba12686/node_modules/ts-node/node_modules:/home/socrates/ghq/github.com/roycehoe/bettertext/node_modules/.pnpm/ts-node@10.4.0_a4f631dedb43a025ec8d07f05ba12686/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.7.0/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@8.7.0/node_modules/acorn/bin/acorn" "$@"
fi
